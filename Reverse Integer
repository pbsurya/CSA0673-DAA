INT_MIN, INT_MAX = -2**31, 2**31 - 1
x = 123

if x < 0:
    sign = -1
    x *= -1
else:
    sign = 1

reversed_x = 0
while x != 0:
    digit = x % 10
    x //= 10
    reversed_x = reversed_x * 10 + digit

reversed_x *= sign
if reversed_x < INT_MIN or reversed_x > INT_MAX:
    print(0)
else:
    print(reversed_x)
